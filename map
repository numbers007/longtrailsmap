	<body>
        <div id="container">
            <div id="sidebar-container">
                <a href='/appalachian-trail/map'>Appalachian Trail Page</a><br>
                <a href='/arizona-trail/map'>Arizona Trail Page</a><br>
                <a href='/pacific-crest-trail/map'>Pacific Crest Trail Page</a><br>
                <br>
                <a href='/whatsnew.html'>What's New?</a><br>
                <a href='/about.html'>About LongTrailMap</a><br>
                <br>
                <input id="search-text" type="search" name="wpt_search" placeholder="Search">
                <button type="button" id="search_go">Go</button>
            </div>
            <div id="map-container">
                <div id="map-buttons">
                    <button type="button" id="sidebar-btn" class="map-buttons">Sidebar</button>
                    <pre id='info'></pre>
                </div>
                <div id="map"></div>
            </div>
        </div>
        <script type="text/javascript">
            var map;            
            var vars = [];
            var markers = [];
            var updateUrl = false;
            
            $.getJSON("http://www.longtrailsmap.net/ltm-trails.geojson", function( data ) {
                markers = data;
            })
            //.fail(function() { alert("Error: Unable to load search."); });
            .fail( function(d, textStatus, error) {
                console.error("getJSON failed, status: " + textStatus + ", error: "+error)
            });

            function searchResultClick(lon, lat, z, short_name) {                
                updateUrl = false;                                                          // don't update the url on moveend event w/ latlon, b/c url should show 'name=[search result name]'.
                map.flyTo({center: [lon, lat], zoom: z, speed: 0.75, curve: 1.5});
                window.history.pushState('page', '', '?name='+short_name);
            };

            $.get("/../common/vars.json", function (vars) {
                mapboxgl.accessToken = vars.mapbox;
                
                $(document).ready(function(){
                    // Mapbox GL JS does not automatically resize & refresh map when size of container changes. Using ResizeSensor to fix. https://stackoverflow.com/a/19418065 
                    new ResizeSensor( $('#map'), function() {console.log('map container resized, calling map.resize() via ResizeSensor.'); map.resize();});

                    $('#sidebar-btn').click(function () {
                        if($('#sidebar-container').css('display') !== 'none') {
                            $('#sidebar-container').css('display', 'none');
                            $('#map-container').css('width', '100%');
                            $('#map').css('width', '100%');                    
                        } else {
                            $('#sidebar-container').css('display', 'block');
                            $('#map-container').css('width', '80%');
                            $('#map').css('width', '80%');                                        
                        }
                    });
                    
                    $("#search-text").keyup(function(event) {
                        if (event.keyCode === 13) {
                            $("#search_go").click();
                        }
                    });                
                    
                    $("#addLocationDescription").keyup(function(event) {
                        if (event.keyCode === 13) {
                            $("#btnAddLocationSubmit").click();
                        }
                    });                

                    $('#search_go').click(function(){
                        var results = [];
                        var items = [];
                        
                        $('#results_list').remove();

                        results = markers.features.filter(function(item) { 
                            return item.properties.searchString.includes($('#search-text').val().toLowerCase());
                        });

                        results.sort(function(a, b) {
                            var textA = a.properties.title.toLowerCase();
                            var textB = b.properties.title.toLowerCase();
                            return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;
                        });
                        
                        $.each(results, function( key, val) {
                            var f = `searchResultClick(${val.geometry.coordinates[0]}, ${val.geometry.coordinates[1]}, ${val.properties.zoom}, \"${val.properties.shortName}\")`;
                            items.push( "<li><a href='#' onClick=\'"+f+"\'>" + val.properties.title + "</a></li>" );
                        });

                        $("<ul/>", {
                            "id": "results_list",
                            "class": "my-new-list",
                            html: items.join("")
                        }).appendTo("#sidebar-container");                        
                    });
                        
                    updateUrl = false;
                    const urlParams = new URLSearchParams(window.location.search);
                    const paramName = urlParams.get('name');
                    const paramLatLon = urlParams.get('latlon');
                    const paramZoom = urlParams.get('zoom');
                        
                    map = new mapboxgl.Map({
                        container: 'map',
                        style: 'mapbox://styles/user9984/cjsfcrcsu2y791fp9lnpjcjkz',                    // "Outdoors (Blank)"
                        //style: 'mapbox://styles/mapbox/satellite-v9',                                 // satellite
                        center: [-96.947, 39.55],
                        zoom: 3.5,
                        minZoom: 0,
                        maxZoom: 22
                    });

                    var nav = new mapboxgl.NavigationControl();
                    map.addControl(nav, 'top-right');

                    var geoLoc = new mapboxgl.GeolocateControl({
                        positionOptions: {
                            enableHighAccuracy: true
                        },
                        trackUserLocation: true
                    });
                    map.addControl(geoLoc, 'top-right');

                    var scale = new mapboxgl.ScaleControl({
                        maxWidth: 80,
                        unit: 'imperial'
                    });
                    map.addControl(scale);
                    // scale.setUnit('metric');
                    
                    // show lnglat of cursor, update whenever cursor moves.
                    map.on('mousemove', function (e) {
                        document.getElementById('info').innerHTML = e.lngLat.lat.toFixed(5) + ", " + e.lngLat.lng.toFixed(5);
                    });
                        
                    map.on('moveend', function (e) {
                        var center = map.getCenter();
                        var zoom = map.getZoom();
                        
                        if(updateUrl == true) {
                            window.history.pushState('page', '', '?latlon=(' + center.lat.toFixed(5) + ',' + center.lng.toFixed(5) +')&zoom=' + zoom.toFixed(2));
                        }

                        updateUrl = true;
                    });
                    
                    map.on('load', function () {
                        $("#search_go").click();                                                    // show search db.
                        
                        var helloPopup = new mapboxgl.Popup({closeButton: true, closeOnClick: true, anchor: 'center'})
                            .setHTML("Welcome to longtrailsmap.net!<br>Click 'About' in the sidebar for more info.<br>Longtrailsmap.net would like to thank the incredible <a target='_blank' rel='noopener noreferrer' href='https://www.mapbox.com/community/'>Mapbox Community Program</a><br>for their generous sponsorship of server costs associated with this site.<br><a target='_blank' rel='noopen noreferrer' href='https://github.com/numbers007'>LongTrailsMap is on GitHub!</a>")
                            .setLngLat([-96.947, 39.55])
                            //.addTo(map)
                            ;

                        var trailNamePopup = new mapboxgl.Popup({ offset: 15, closeButton: false });

                        var addLocationLat = "";
                        var addLocationLon = "";
                        
                        map.doubleClickZoom.disable();
                        
                        if(window.location.search == "") {
                            helloPopup.addTo(map);
                        } else {
                            var results = [];
                            
                            if(paramName !== "" && paramName !== null) {
                                results = markers.features.filter(function(item) { 
                                    return item.properties.searchString.includes(paramName);
                                });

                                var lon = results[0].geometry.coordinates[0];
                                var lat = results[0].geometry.coordinates[1];
                                var z = results[0].properties.zoom;
                                    
                                updateUrl = false;
                                map.flyTo({center: [lon, lat], zoom:z, speed: 0.5, curve: 1.82});

                            } else {
                                if(paramLatLon !== null && paramZoom !== null) {
                                    const lat = parseFloat(paramLatLon.replace('(','').replace(')','').split(',')[0]);
                                    const lon = parseFloat(paramLatLon.replace('(','').replace(')','').split(',')[1]);
                                    
                                    map.flyTo({center: [lon, lat], zoom: parseFloat(paramZoom), speed:0.5, curve: 1.82});
                                } else {
                                    if(paramLatLon != null) {
                                        const lat = parseFloat(paramLatLon.replace('(','').replace(')','').split(',')[0]);
                                        const lon = parseFloat(paramLatLon.replace('(','').replace(')','').split(',')[1]);
                                        
                                        map.flyTo({center: [lon, lat], zoom: 16, speed:0.5, curve: 1.82});
                                    } else {
                                        if(paramZoom !== null) {
                                            map.zoomTo(paramZoom);
                                        }
                                    }
                                }
                            }
                        };

                        map.on('dragstart', function() {
                            helloPopup.remove();
                        });

                        map.addLayer({
                            'id': 'north-umpqua-trail',				            // 'id' is arbitrary
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.0vxi4g01'        	    // this s/b a tileset 'map id'. must be an existing resource.
                            },
                            'source-layer': 'tracks',				            // from tileset page.
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'north-umpqua-trail', function(e) {
                            map.setPaintProperty('north-umpqua-trail', 'line-width', 4);
            
                            trailNamePopup.setText('North Umpqua Trail');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'north-umpqua-trail', function() {
                            map.setPaintProperty('north-umpqua-trail', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                            'id': 'big-bend-100',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck23omv07008u2tml4xivn7wl-9789w'
                            },
                            'source-layer': 'big-bend-100',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'big-bend-100', function(e) {
                            map.setPaintProperty('big-bend-100', 'line-width', 4);
            
                            trailNamePopup.setText('Big Bend 100');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'big-bend-100', function() {
                            map.setPaintProperty('big-bend-100', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                          'id': 'ozark-trail-3',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck0pvwuzz0h0o2opupyqgfty6-9syw6'        
                            },
                            'source-layer': 'ozark-trail-3',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'ozark-trail-3', function(e) {
                            map.setPaintProperty('ozark-trail-3', 'line-width', 4);

                            trailNamePopup.setText('Ozark Trail');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'ozark-trail-3', function() {
                            map.setPaintProperty('ozark-trail-3', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                          'id': 'e2-belgium',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck12puko8h54g2ip7ht9tp0hq-7q9jn'        
                            },
                            'source-layer': 'e2-belgium',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'e2-belgium', function(e) {
                            map.setPaintProperty('e2-belgium', 'line-width', 4);
                            map.setPaintProperty('e2-france', 'line-width', 4);
                            map.setPaintProperty('e2-netherlands', 'line-width', 4);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 4);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 4);

                            trailNamePopup.setText('E2');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'e2-belgium', function() {
                            map.setPaintProperty('e2-belgium', 'line-width', 2);
                            map.setPaintProperty('e2-france', 'line-width', 2);
                            map.setPaintProperty('e2-netherlands', 'line-width', 2);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 2);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                          'id': 'e2-france',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck12pvcr806d52nmsejpxnunl-5fpuv'        
                            },
                            'source-layer': 'e2-france',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'e2-france', function(e) {
                            map.setPaintProperty('e2-belgium', 'line-width', 4);
                            map.setPaintProperty('e2-france', 'line-width', 4);
                            map.setPaintProperty('e2-netherlands', 'line-width', 4);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 4);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 4);

                            trailNamePopup.setText('E2');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'e2-france', function() {
                            map.setPaintProperty('e2-belgium', 'line-width', 2);
                            map.setPaintProperty('e2-france', 'line-width', 2);
                            map.setPaintProperty('e2-netherlands', 'line-width', 2);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 2);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                          'id': 'e2-netherlands',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck12pwogz1mgk2ts2owpgm42a-50ci7'        
                            },
                            'source-layer': 'e2-netherlands',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'e2-netherlands', function(e) {
                            map.setPaintProperty('e2-belgium', 'line-width', 4);
                            map.setPaintProperty('e2-france', 'line-width', 4);
                            map.setPaintProperty('e2-netherlands', 'line-width', 4);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 4);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 4);

                            trailNamePopup.setText('E2');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'e2-netherlands', function() {
                            map.setPaintProperty('e2-belgium', 'line-width', 2);
                            map.setPaintProperty('e2-france', 'line-width', 2);
                            map.setPaintProperty('e2-netherlands', 'line-width', 2);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 2);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                          'id': 'e2-united-kingdom',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck12pxf9002cw2inplqu84gse-2wbba'        
                            },
                            'source-layer': 'e2-united-kingdom',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'e2-united-kingdom', function(e) {
                            map.setPaintProperty('e2-belgium', 'line-width', 4);
                            map.setPaintProperty('e2-france', 'line-width', 4);
                            map.setPaintProperty('e2-netherlands', 'line-width', 4);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 4);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 4);

                            trailNamePopup.setText('E2');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'e2-united-kingdom', function() {
                            map.setPaintProperty('e2-belgium', 'line-width', 2);
                            map.setPaintProperty('e2-france', 'line-width', 2);
                            map.setPaintProperty('e2-netherlands', 'line-width', 2);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 2);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 2);

                            trailNamePopup.remove();
                        });


                        map.addLayer({
                          'id': 'e2-luxemburg',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck12q1el902e82inpcpoyhlh3-2hru3'        
                            },
                            'source-layer': 'e2-luxemburg',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'e2-luxemburg', function(e) {
                            map.setPaintProperty('e2-belgium', 'line-width', 4);
                            map.setPaintProperty('e2-france', 'line-width', 4);
                            map.setPaintProperty('e2-netherlands', 'line-width', 4);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 4);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 4);

                            trailNamePopup.setText('E2');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'e2-luxemburg', function() {
                            map.setPaintProperty('e2-belgium', 'line-width', 2);
                            map.setPaintProperty('e2-france', 'line-width', 2);
                            map.setPaintProperty('e2-netherlands', 'line-width', 2);
                            map.setPaintProperty('e2-united-kingdom', 'line-width', 2);
                            map.setPaintProperty('e2-luxemburg', 'line-width', 2);

                            trailNamePopup.remove();
                        });

                        map.addLayer({
                          'id': 'grand-italian-trail-01',
                            'type': 'line',
                            'source': {
                                'type': 'vector',
                                'url': 'mapbox://user9984.ck0opuh7y37es2tq7tgaaa8nl-2ke59'        
                            },
                            'source-layer': 'grand-italian-trail-01',
                            'layout': {
                                'visibility': 'visible'
                            },
                            'paint': {
                                'line-color': '#ff0901',
                                'line-width': 2
                            }
                        });
                        map.on('mousemove', 'grand-italian-trail-01', function(e) {
                            map.setPaintProperty('grand-italian-trail-01', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-02', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-03', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-04', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-05', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-06', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-07', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-08', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-09', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-10', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-11', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-12', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-13', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-14', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-15', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-16', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-17', 'line-width', 4);
                            map.setPaintProperty('grand-italian-trail-18', 'line-width', 4);

                            trailNamePopup.setText('Grand Italian Trail');
                            trailNamePopup
                                .setLngLat(e.lngLat)
                                .addTo(map);
                        });
                        map.on('mouseleave', 'grand-italian-trail-01', function() {
                            map.setPaintProperty('grand-italian-trail-01', 'line-width', 2);
